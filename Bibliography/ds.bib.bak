@article{Hooper:2008hv,
author = {Hooper, Dan and Zurek, Kathryn M},
title = {{Natural supersymmetric model with MeV dark matter}},
journal = {Phys. Rev. D},
year = {2008},
volume = {77},
number = {8},
pages = {087302},
month = apr,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.77.087302},
language = {English},
rating = {0},
date-added = {2018-04-20T07:40:19GMT},
date-modified = {2018-04-20T07:51:39GMT},
abstract = {It has previously been proposed that annihilating dark matter particles with MeV-scale masses could be responsible for the flux of 511 keV photons observed from the region of the Galactic Bulge. The conventional wisdom, however, is that it is very challenging to construct a viable particle physics model containing MeV dark matter. In this paper, we challenge this conclusion by describing a simple and natural supersymmetric model in which the lightest supersymmetric particle naturally has a MeV-scale mass and the other phenomenological properties required to generate the 511 keV emission. In particular, the small ($\ensuremath{\sim}{10}^{\ensuremath{-}5}$) effective couplings between dark matter and the standard model fermions required in this scenario naturally lead to radiative corrections that generate MeV-scale masses for both the dark matter candidate and the mediator particle.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.77.087302},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/04/04DB24CF-C913-4EAB-9830-B262C9DD23AB.pdf},
