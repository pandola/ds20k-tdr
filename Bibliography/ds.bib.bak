@article{Hunt:2004js,
author = {Hunt, D C and Tousignant, O and Rowlands, John A},
title = {{Evaluation of the imaging properties of an amorphous selenium-based flat panel detector for digital fluoroscopy}},
journal = {Med. Phys.},
year = {2004},
volume = {31},
number = {5},
pages = {1166--1175},
publisher = {American Association of Physicists in Medicine},
doi = {10.1118/1.1707755},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-06-21T14:25:28GMT},
date-modified = {2016-08-05T16:13:41GMT},
abstract = {The imaging performance of an amorphous selenium (a- Se ) flat-panel detector for digital fluoroscopy was experimentally evaluated using the spatial frequency dependent modulation transfer function(MTF),noise power spectrum (NPS), and detective quantum efficiency (DQE). These parameters were investigated at beam qualities and exposures within the range typical of gastrointestinal fluoroscopic imaging (∼0.1{\textendash}10 $\mu$ R , 75 kV). The investigation does not take into consideration the detector cover, which in clinical use will lower the DQE measured here by its percent attenuation. The MTF was found to be less than the expected aperture response and the NPS was not white which together indicate presampling blurring. The cause of this blurring was attributed to charge trapping at the interface between two different layers of the a- Se . The effect on the DQE was also consistent with presampling blur, which reduces the aliasing in the NPS and thereby reduces the spatial frequency dependence of the DQE. (The DQE was independent of spatial frequency from 0.12 to 0.73  mm {\textminus}1 due to antialiasing of the NPS.) Moreover, the first zero of the measured MTF and the aperture response appeared at the same spatial frequency (6.66  mm {\textminus}1 for a pixel of 150 $\mu$m). Hence, the geometric fill factor (77%) was increased to an effective fill factor of 99{\textpm}1%. A large scale (∼32  pixels ) correlation in the noise due to the configuration of the readout electronics caused increased noise power in the gate line NPS at low spatial frequency ({\&}lt;0.1  mm {\textminus}1 ). The DQE (f=0) was exposure independent over a large range of exposures but became exposure dependent at low exposures due to the electronic noise.},
url = {http://scitation.aip.org/content/aapm/journal/medphys/31/5/10.1118/1.1707755},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/E7/E7C4BB1E-8A40-44EB-A7A3-984DE7162DD0.pdf},
file = {{E7C4BB1E-8A40-44EB-A7A3-984DE7162DD0.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/E7/E7C4BB1E-8A40-44EB-A7A3-984DE7162DD0.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1118/1.1707755}}
}

