@article{Saldanha:2019vx,
author = {Saldanha, Richard N and Back, Henning O and Tsang, R H M and Alexander, Thomas and Elliott, S R and Ferrara, S and Mace, E and Overman, C and Zalavadia, M},
title = {{Cosmogenic production of $^{39}$Ar and $^{37}$Ar in argon}},
journal = {arXiv},
year = {2019},
eprint = {1902.09072v1},
eprinttype = {arxiv},
eprintclass = {nucl-ex},
month = feb,
annote = {13 pages, 9 figures, 6 tables},
rating = {0},
date-added = {2019-03-18T06:50:21GMT},
date-modified = {2019-04-21T16:26:41GMT},
abstract = {We have experimentally determined the production rate of 39Ar and 37Ar due to cosmic ray neutron interactions in argon at sea level. Understanding these production rates is important for argon-based dark matter experiments that plan to utilize argon extracted from deep underground because it is imperative to know what the ingrowth of 39Ar will be during the production, transport, and storage of the underground argon. These measurements also allow for the prediction of 39Ar and 37Ar concentrations in the atmosphere which can be used to determine the presence of other sources of these isotopes. Through controlled irradiation with a neutron beam that mimics the cosmic ray neutron spectrum, followed by direct counting of 39Ar and 37Ar decays with sensitive ultra-low background proportional counters, we determined that the production rate from cosmic ray neutrons at sea-level is expected to be (759  122) atoms/kg/day for 39Ar, and (51.0  7.0) atoms/kg/day for 37Ar. We also performed a survey of the alternate production mechanisms based on the state-of-knowledge of the associated cross-sections to obtain a total sea-level cosmic ray production rate of (1048  126) atoms/kg/day for 39Ar, (56.8  7.1) atoms/kg/day for 37Ar in underground argon, and (92  13) atoms/kg/day for 37Ar in atmospheric argon.},
url = {http://arxiv.org/abs/1902.09072v1},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/28/28E7749D-5ECC-493B-9D63-3AEE439364EE.pdf},
file = {{28E7749D-5ECC-493B-9D63-3AEE439364EE.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/28/28E7749D-5ECC-493B-9D63-3AEE439364EE.pdf:application/pdf}},
