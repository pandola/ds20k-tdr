title = {{A new method of measuring $^{81}$Kr and $^{85}$Kr abundances in environmental samples}},
journal = {Geophys. Res. Lett.},
year = {2003},
volume = {30},
number = {20},
pages = {2068},
month = oct,
doi = {10.1029/2003GL018293},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-09-06T08:28:42GMT},
date-modified = {2017-02-26T15:31:33GMT},
abstract = {We demonstrate a new method for determining the 81Kr/Kr ratio in environmental samples based upon two measurements: the 85Kr/81Kr ratio measured by Atom Trap Trace Analysis (ATTA) and the 85Kr/Kr ratio measured by Low-Level Counting (LLC).  This method can be used to determine the mean residence time of groundwater in the range of 10^5 {\textendash} 10^6 a.  It requires a sample of 100 ml STP of Kr extracted from approximately two tons of water.  With modern atmospheric Kr samples, we demonstrate that the ratios measured by ATTA and LLC are directly proportional to each other within the measurement error of {\textpm}10%; we calibrate the 81Kr/Kr ratio of modern air measured using this method; and we show that the 81Kr/Kr ratios of samples extracted from air before and after the development of the nuclear industry are identical within the measurement error.},
url = {http://doi.wiley.com/10.1029/2003GL018293},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/42/4277A96B-2F0D-4863-B128-F07BB742B6C1.pdf},
