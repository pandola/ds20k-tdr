@article{Bevington:1961hs,
author = {Bevington, P R and Rolland, W W and Lewis, H W},
title = {{Relative Yields of Neutron Groups from the Li$^{7}$(\emph{p},\emph{n})Be$^{7}$,~Be$^{7*}$ Reactions}},
journal = {Phys. Rev.},
year = {1961},
volume = {121},
number = {3},
pages = {871--876},
month = feb,
publisher = {American Physical Society},
doi = {10.1103/PhysRev.121.871},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-07-29T07:17:23GMT},
date-modified = {2016-07-29T07:18:37GMT},
abstract = {The relative yields of the two groups of neutrons from the ${\mathrm{Li}}^{7}(p, n){\mathrm{Be}}^{7}, {\mathrm{Be}}^{7*}$ reaction, leading to the ground state and the 430-kev state of ${\mathrm{Be}}^{7}$, have been measured with a time-of-flight system, using pre-acceleration pulsing of the accelerator beam. Data were taken at 30\ifmmode^\circ\else\textdegree\fi{} intervals between 0\ifmmode^\circ\else\textdegree\fi{} and 150\ifmmode^\circ\else\textdegree\fi{} for proton energies high enough to produce ($p, {n}^{\ensuremath{'}}$) neutrons above the detection threshold (300 kev). Since the yield of the ($p, n$) reaction is more highly peaked in the forward direction than that of the ($p, {n}^{\ensuremath{'}}$) reaction, the ratio of the ($p, {n}^{\ensuremath{'}}$) to ($p, n$) intensities grows with increasing angle, severely limiting the usefulness of the ($p, n$) reaction as a neutron source above the ($p, {n}^{\ensuremath{'}}$) threshold at back angles. Absolute differential and total cross sections for both groups have been calculated from the data. A comparison with theory for total cross sections and angular distributions suggests the existence of three previously unidentified levels in ${\mathrm{Be}}^{8}$. One level, with ${J}^{\ensuremath{\pi}}={1}^{\ensuremath{-}}$, near the threshold for the ($p, {n}^{\ensuremath{'}}$) reaction, is responsible for the fast rise in the ($p, {n}^{\ensuremath{'}}$) total cross section near threshold. A second level, corresponding to an incident proton energy of about 3.0 Mev, does not contribute significantly to the yield of the ($p, {n}^{\ensuremath{'}}$) reaction; the data are consistent with an assignment of ${J}^{\ensuremath{\pi}}={1}^{+}$ and a total width of 1 Mev. The bulk of the total cross-section curve for the ($p, {n}^{\ensuremath{'}}$) reaction has been fitted by assuming a ${1}^{+}$ level corresponding to an incident proton energy of 3.5 Mev, with ${{\ensuremath{\gamma}}_{n}}^{2}\ensuremath{\simeq}{{\ensuremath{\gamma}}_{p}}^{2}$, ${{\ensuremath{\gamma}}_{{n}^{\ensuremath{'}}}}^{2}\ensuremath{\simeq}5{{\ensuremath{\gamma}}_{p}}^{2}$, and ${{\ensuremath{\gamma}}_{{p}^{\ensuremath{'}}}}^{2}\ensuremath{\ll}{{\ensuremath{\gamma}}_{p}}^{2}$.},
url = {http://link.aps.org/doi/10.1103/PhysRev.121.871},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/42/42546BFD-7ABD-428D-858A-43ECDB5C18B4.pdf},
file = {{42546BFD-7ABD-428D-858A-43ECDB5C18B4.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/42/42546BFD-7ABD-428D-858A-43ECDB5C18B4.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRev.121.871}}
}

