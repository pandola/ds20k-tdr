title = {{Theory of Microplasma Instability in Silicon}},
journal = {J. Appl. Phys.},
year = {1961},
volume = {32},
number = {6},
pages = {983--995},
publisher = {AIP Publishing},
doi = {10.1063/1.1736199},
language = {English},
rating = {0},
date-added = {2016-07-01T08:18:18GMT},
date-modified = {2016-08-05T13:18:01GMT},
abstract = {A statistical theory is presented to explain microplasma instability at the onset of avalanche in reversebiased silicon linearly graded and step junctions. An expression is derived which relates the turnoff probability of the microplasma to the differential resistance of the diode in its conducting state and to other physically measurable diode parameters. Measurements of the turnoff probability as a function of the pulse current are presented for several diodes and are shown to agree well with the derived theory. To explain the turnon probability, three expressions, each involving slightly different approximations, are derived for the probability that a carrier entering the breakdown region will initiate an avalanche. In each case, this probability is found to be proportional to the excess of the applied voltage over a uniquely definable sustaining voltage Vs , in poor agreement with experiment. The various mechanisms which determine the diode{\&}apos;s differential impedance in the conducting state are discussed and approximate expressions for the contributions of each mechanism to the differential impedance are derived. Multilevel pulses, previously interpreted as indicating more than one conducting state for a microplasma, are explained in terms of parallel breakdowns of more than one microplasma.},
url = {http://scitation.aip.org/content/aip/journal/jap/32/6/10.1063/1.1736199},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/9A/9ADFE172-79F1-4A8B-A3AE-4163397F86A5.pdf},
file = {{9ADFE172-79F1-4A8B-A3AE-4163397F86A5.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/9A/9ADFE172-79F1-4A8B-A3AE-4163397F86A5.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1063/1.1736199}}
}

