file = {{8CE3AC07-1EDB-4977-9C4B-D06FF7D403E1.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/8C/8CE3AC07-1EDB-4977-9C4B-D06FF7D403E1.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/es201192u}}
