file = {{8B312726-2602-4A5A-B27B-ED321C568385.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/8B/8B312726-2602-4A5A-B27B-ED321C568385.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.95.082002}}
}

