@article{Kuflik:2010kd,
author = {Kuflik, Eric and Pierce, Aaron and Zurek, Kathryn M},
title = {{Light neutralinos with large scattering cross sections in the minimal supersymmetric standard model}},
journal = {Phys. Rev. D},
year = {2010},
volume = {81},
number = {11},
pages = {111701},
month = jun,
doi = {10.1103/PhysRevD.81.111701},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-08-20T14:38:11GMT},
date-modified = {2015-09-07T19:53:16GMT},
abstract = {Motivated by recent data from CoGeNT and the DAMA annual modulation signal, we discuss collider constraints on the minimal supersymmetric standard model neutralino dark matter with mass in the 5\char21{}15 GeV range. The lightest superpartner (LSP) would be a bino with a small Higgsino admixture. Maximization of the dark matter nucleon scattering cross section for such a weakly interacting massive particle requires a light Higgs boson with $\mathrm{tan}﻿\ensuremath{\beta}$ enhanced couplings. Limits on the invisible width of the $Z$ boson, combined with the rare decays ${B}^{\ifmmode\pm\else\textpm\fi{}}\ensuremath{\rightarrow}\ensuremath{\tau}\ensuremath{\nu}$, and the ratio $B\ensuremath{\rightarrow}D\ensuremath{\tau}\ensuremath{\nu}/B\ensuremath{\rightarrow}D\ensuremath{\ell}\ensuremath{\nu}$, constrain cross sections to be below ${\ensuremath{\sigma}}_{n}\ensuremath{\lesssim}5\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}42}\text{ }\text{ }{\mathrm{cm}}^{2}$. This indicates a higher local dark matter density than is usually assumed by a factor of roughly 6 necessary to explain the CoGeNT excess. This scenario also requires a light charged Higgs boson, which can give substantial contributions to rare decays such as $b\ensuremath{\rightarrow}s\ensuremath{\gamma}$ and $t\ensuremath{\rightarrow}b{H}^{+}$. We also discuss the impact of Tevatron searches for Higgs bosons at large $\mathrm{tan}﻿\ensuremath{\beta}$.},
url = {http://link.aps.org/doi/10.1103/PhysRevD.81.111701},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/C0/C0FFAED5-631E-4EE0-88F1-8F2F81C3F06C.pdf},
file = {{C0FFAED5-631E-4EE0-88F1-8F2F81C3F06C.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/C0/C0FFAED5-631E-4EE0-88F1-8F2F81C3F06C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.81.111701}}
}

