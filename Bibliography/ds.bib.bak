@article{Agostini:2018bt,
author = {Agostini, M and Altenm{\"u}ller, K and Appel, S and Atroshchenko, V and Bagdasarian, Z and Basilico, D and Bellini, Gianpaolo and Benziger, Jay and Bick, Daniel and Bonfini, Giuseppe and Bravo, David and Caccianiga, Barbara and Calaprice, Frank P and Caminata, Alessio and Caprioli, S and Carlini, Marco and Cavalcante, Paolo and Chepurnov, Alexander and Choi, K and Collica, L and D'Angelo, Davide and Davini, Stefano and Derbin, Alexander V and Ding, X F and Di Ludovico, Antonio and Di Noto, L and Dratchnev, Iliya and Fomenko, Kirill and Formozov, A and Franco, Davide and Gabriele, Federico and Galbiati, Cristiano and Ghiano, Chiara and Giammarchi, Marco G and Goretti, Augusto M and Gromov, Maxim and Guffanti, D and Hagner, Caren and Houdy, T and Hungerford, Ed V and Ianni, Aldo and Ianni, Andrea M and Jany, A and Jeschke, D and Kobychev, Vladislav V and Korablev, Denis and Korga, George and Kryn, Daniel and Laubenstein, Matthias and Litvinovich, Evgeniy and Lombardi, Francesco and Lombardi, Paolo and Ludhova, Livia and Lukyanchenko, Georgy and Machulin, Igor N and Manuzio, Giulio and Marcocci, Simone and Martyn, J and Meroni, Emanuela and Meyer, Mikko and Miramonti, Lino and Misiaszek, Marcin and Muratova, Valentina and Neumair, B and Oberauer, Lothar and Opitz, B and Orekhov, V and Ortica, Fausto and Pallavicini, Marco and Papp, Laszlo and Penek, O and Pilipenko, N and Pocar, Andrea and Porcelli, A and Raikov, G and Ranucci, Gioacchino and Razeto, Alessandro and Re, Alessandra and Redchuk, M and Romani, Aldo and Roncin, R and Rossi, Nicola and Sch{\"o}nert, Stefan and Semenov, Dmitrii A and Skorokhvatov, Mikhail D and Smirnov, Oleg and Sotnikov, Albert and Stokes, L F F and Suvorov, Yury and Tartaglia, Roberto and Testera, Gemma and Thurn, J and Toropova, M and Unzhakov, Evgeniy and Villante, F L and Vishneva, Alina and Vogelaar, R Bruce and von Feilitzsch, Franz and Wang, Hanguo and Weinz, S and W{\'o}jcik, Marcin and Wurm, Michael and Yokley, Z and Zaimidoroga, Oleg and Zavatarelli, Sandra and Zuber, Kai and Zuzel, Grzegorz},
title = {{Comprehensive measurement of pp-chain solar neutrinos}},
journal = {Nature},
year = {2018},
volume = {562},
number = {7728},
pages = {505--510},
month = oct,
publisher = {Nature Publishing Group},
affiliation = {The Borexino Collaboration},
doi = {10.1038/s41586-018-0624-y},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-10-29T22:41:12GMT},
date-modified = {2018-11-23T10:33:46GMT},
abstract = {About 99 per cent of solar energy is produced through sequences of nuclear reactions that convert hydrogen into helium, starting from the fusion of two protons (the pp chain). The neutrinos emitted by five of these reactions represent a unique probe of the Sun{\textquoteright}s internal working and, at the same time, offer an intense natural neutrino beam for fundamental physics. Here we report a complete study of the pp chain. We measure the neutrino{\textendash}electron elastic-scattering~rates for neutrinos produced by four reactions of the chain: the initial proton{\textendash}proton~fusion, the electron-capture decay of beryllium-7, the three-body proton{\textendash}electron{\textendash}proton (pep) fusion,~here measured with the highest precision so far achieved, and the boron-8 beta decay, measured with the lowest energy~threshold. We also set a limit on the neutrino flux produced by the 3He{\textendash}proton fusion (hep). These measurements provide a direct determination of the relative intensity of the two primary terminations of the pp chain (pp-I and pp-II) and an indication that the temperature profile in the Sun is more compatible with solar models that assume high surface metallicity. We also determine the survival probability of solar electron neutrinos at different energies, thus probing simultaneously and with high precision the neutrino~flavour-conversion paradigm, both in vacuum and in matter-dominated regimes.},
url = {http://www.nature.com/articles/s41586-018-0624-y},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/2B/2B1A68D9-37EB-467D-8330-60138DA7BC3F.pdf},
file = {{2B1A68D9-37EB-467D-8330-60138DA7BC3F.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/2B/2B1A68D9-37EB-467D-8330-60138DA7BC3F.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/s41586-018-0624-y}}
}

