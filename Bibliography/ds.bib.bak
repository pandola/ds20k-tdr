date-added = {2015-08-22T19:31:36GMT},
date-modified = {2017-04-09T13:38:10GMT},
abstract = {Vapor{\textendash}liquid and vapor{\textendash}solid isotopic fractionation of noble gases and their mixtures are studied by molecular-based simulation of atomic Lennard-Jones fluids. The temperature dependence of the fractionation factors for 36 Ar / 40 Ar , 20 Ne / 22 Ne , 80 Kr / 84 Kr and the composition dependence of the corresponding fractionation factors for binary Ar{\textendash}Kr mixtures as predicted by simulation are compared with the existing experimental data, to assess the accuracy of  2 -order Kirkwood{\textendash}Wigner free energy expansion for specific Lennard-Jones parametrizations. Predictions of the fractionation factors for other isotopic pairs, including 20 Ne / 21 Ne , and 132 Xe / 136 Xe as well as tests of some premises behind the microscopic interpretation of the fractionation factors are also given.},
url = {http://scitation.aip.org/content/aip/journal/jcp/119/8/10.1063/1.1594178},
