@article{Bechet:2017il,
author = {B{\'e}chet, Quentin and Laviale, Martin and Arsapin, Nicolas and Bonnefond, Hubert and Bernard, Olivier},
title = {{Modeling the impact of high temperatures on microalgal viability and photosynthetic activity}},
journal = {Biotechnol Biofuels},
year = {2017},
volume = {10},
number = {1},
pages = {217},
month = may,
publisher = {BioMed Central},
doi = {10.1186/s13068-017-0823-z},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-04-12T22:31:24GMT},
date-modified = {2019-01-24T21:12:34GMT},
abstract = {Culture collapse due to high temperatures can significantly impact the profitability of outdoor algal cultivation systems. The objective of this study was to model for the first time the impact of high temperatures on algal activity and viability. Viability measurements on Dunaliella salina cultures were based on cytometry with two fluorescent markers (erythrosine and fluorescein di-acetate), and photosynthetic activity was measured by Pulse Amplitude Modulation{\&}nbsp;(PAM) fluorometry. Kinetic studies revealed that viability and activity losses during exposure to high temperatures could be described by a Weibull model. Both mortality and activity were shown to be functions of the thermal dose received by the algae, defined as the product of duration of exposure to high temperatures and an exponential function of temperature. Simulations at five climatic locations revealed that culture collapse due to high temperatures could impact productivity of D. salina in non-temperature-controlled outdoor photobioreactors by 35 and 40% in arid and Mediterranean climates, respectively. The model developed in this study can be used to forecast the impact of high temperatures on algal biofuel productivity. When coupled with models predicting the temperature of outdoor cultivation systems, this model can also be used to select the best combination of location, system geometry, and algal species to minimize the risks of culture collapse and therefore maximize biofuel productivity.},
url = {http://biotechnologyforbiofuels.biomedcentral.com/articles/10.1186/s13068-017-0823-z},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/80/803C4F90-E827-49D1-8067-97F54928C465.pdf},
file = {{803C4F90-E827-49D1-8067-97F54928C465.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/80/803C4F90-E827-49D1-8067-97F54928C465.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/s13068-017-0823-z}}
}

