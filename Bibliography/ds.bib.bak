title = {{Feasibility of time-of-flight reconstruction in positron emission tomography.}},
journal = {J. Nucl. Med.},
year = {1980},
volume = {21},
number = {11},
pages = {1095--1097},
month = nov,
pmid = {6968822},
language = {English},
read = {Yes},
