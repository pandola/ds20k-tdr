journal = {J. Chem. Phys.},
year = {1979},
volume = {71},
number = {6},
pages = {2676--2681},
publisher = {American Institute of Physics},
doi = {10.1063/1.438625},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-05-04T15:49:17GMT},
date-modified = {2017-05-04T16:01:00GMT},
abstract = {We report measurements of lifetimes of trapped excitons in condensed argon and xenon. For argon, decays are qualitatively similar to those found in the gas phases. For xenon, we find by both spectroscopy and the time dependence of the fluorescence that the n=1 and n=2 $\Gamma$ (3/2) excitons radiating at 175 and 164 nm, respectively, are present for all temperatures from 80{\textendash}133 K. The measured decay times are found to be independent of temperature and density. We discuss the validity of decay time measurements when using electron beams for excitation.},
url = {http://aip.scitation.org/doi/10.1063/1.438625},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/27/27C548A6-C9B3-4F86-B2DA-F6003E932241.pdf},
