title = {{Dark Matter Search Results from the PICO-2L C$_{3}$F$_{8}$ Bubble Chamber}},
journal = {Phys. Rev. Lett.},
year = {2015},
volume = {114},
number = {23},
pages = {231302},
month = jun,
affiliation = {The PICO Collaboration},
doi = {10.1103/PhysRevLett.114.231302},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-08-19T15:49:25GMT},
date-modified = {2015-10-01T16:21:43GMT},
abstract = {New data are reported from the operation of a 2 liter ${\mathrm{C}}_{3}{\mathrm{F}}_{8}$ bubble chamber in the SNOLAB underground laboratory, with a total exposure of 211.5 kg days at four different energy thresholds below 10 keV. These data show that ${\mathrm{C}}_{3}{\mathrm{F}}_{8}$ provides excellent electron-recoil and alpha rejection capabilities at very low thresholds. The chamber exhibits an electron-recoil sensitivity of ${\&}lt;3.5\ifmmode\times\else\texttimes\fi{}1{0}^{\ensuremath{-}10}$ and an alpha rejection factor of ${\&}gt;98.2%$. These data also include the first observation of a dependence of acoustic signal on alpha energy. Twelve single nuclear recoil event candidates were observed during the run. The candidate events exhibit timing characteristics that are not consistent with the hypothesis of a uniform time distribution, and no evidence for a dark matter signal is claimed. These data provide the most sensitive direct detection constraints on WIMP-proton spin-dependent scattering to date, with significant sensitivity at low WIMP masses for spin-independent WIMP-nucleon scattering.},
url = {http://link.aps.org/doi/10.1103/PhysRevLett.114.231302},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/2D/2D486B66-FE38-427E-95D9-A82EE82D344B.pdf},
file = {{2D486B66-FE38-427E-95D9-A82EE82D344B.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/2D/2D486B66-FE38-427E-95D9-A82EE82D344B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevLett.114.231302}}
}

