@article{Stanford:2018un,
author = {Stanford, Christopher and Westerdale, Shawn S and Xu, Jingke and Calaprice, Frank P},
title = {{Surface background suppression in liquid argon dark matter detectors using a newly discovered time component of tetraphenyl-butadiene scintillation}},
journal = {Phys. Rev. D},
year = {2018},
volume = {98},
number = {6},
pages = {062002},
month = sep,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.98.062002},
language = {English},
rating = {0},
date-added = {2018-10-16T15:05:06GMT},
date-modified = {2018-10-16T15:10:04GMT},
abstract = {Decays of radioisotopes on inner detector surfaces can pose a major background concern for the direct detection of dark matter. While these backgrounds are conventionally mitigated with position cuts, these cuts reduce the exposure of the detector by decreasing the sensitive mass, and uncertainty in position determination may make it impossible to adequately remove such events in certain detectors. In this paper, we provide a new technique for substantially reducing these surface backgrounds in liquid argon (LAr) detectors, independent of position cuts. These detectors typically use a coating of tetraphenyl-butadiene (TPB) on the inner surfaces as a wavelength shifter to convert vacuum ultraviolet (VUV) LAr scintillation light to the visible spectrum. We find that TPB scintillation contains a component with a previously unreported exceptionally long lifetime ($\ensuremath{\sim}\mathrm{ms}$). We discovered that this component differs significantly in magnitude between alpha, beta, and VUV excitation, which enables the use of pulse shape discrimination to suppress surface backgrounds by more than a factor of $1{0}^{3}$ with negligible loss of dark matter sensitivity. We also discuss how this technique can be extended beyond just LAr experiments.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.98.062002},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/56/56815D1A-B846-4EF2-8F4E-7B3C17AE375C.pdf},
file = {{56815D1A-B846-4EF2-8F4E-7B3C17AE375C.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/56/56815D1A-B846-4EF2-8F4E-7B3C17AE375C.pdf:application/pdf;56815D1A-B846-4EF2-8F4E-7B3C17AE375C.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/56/56815D1A-B846-4EF2-8F4E-7B3C17AE375C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.98.062002}}
}

