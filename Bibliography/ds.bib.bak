@article{Tarallo:2014kd,
author = {Tarallo, M G and Mazzoni, T and Poli, N and Sutyrin, D V and Zhang, X and Tino, Guglielmo M},
title = {{Test of Einstein Equivalence Principle for 0-Spin and Half-Integer-Spin Atoms: Search for Spin-Gravity Coupling Effects}},
journal = {Phys. Rev. Lett.},
year = {2014},
volume = {113},
number = {2},
pages = {023005},
month = jul,
doi = {10.1103/PhysRevLett.113.023005},
language = {English},
rating = {0},
date-added = {2015-09-21T13:26:36GMT},
date-modified = {2016-08-05T16:13:43GMT},
abstract = {We report on a conceptually new test of the equivalence principle performed by measuring the acceleration in Earth's gravity field of two isotopes of strontium atoms, namely, the bosonic $^{88}\mathrm{Sr}$ isotope which has no spin versus the fermionic $^{87}\mathrm{Sr}$ isotope which has a half-integer spin. The effect of gravity on the two atomic species has been probed by means of a precision differential measurement of the Bloch frequency for the two atomic matter waves in a vertical optical lattice. We obtain the values $\ensuremath{\eta}=(0.2\ifmmode\pm\else\textpm\fi{}1.6)\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}7}$ for the E\"otv\"os parameter and $k=(0.5\ifmmode\pm\else\textpm\fi{}1.1)\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}7}$ for the coupling between nuclear spin and gravity. This is the first reported experimental test of the equivalence principle for bosonic and fermionic particles and opens a new way to the search for the predicted spin-gravity coupling effects.},
url = {http://link.aps.org/doi/10.1103/PhysRevLett.113.023005},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/00/00D90C61-F720-45D2-9F9D-02366B2BAF62.pdf},
file = {{00D90C61-F720-45D2-9F9D-02366B2BAF62.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/00/00D90C61-F720-45D2-9F9D-02366B2BAF62.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevLett.113.023005}}
}

