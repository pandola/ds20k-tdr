file = {{645AAD2D-3E3D-42FC-AD14-BB01E0574719.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/64/645AAD2D-3E3D-42FC-AD14-BB01E0574719.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevLett.108.051302}}
}

