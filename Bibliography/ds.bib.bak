@article{Billard:2014cx,
author = {Billard, J and Figueroa-Feliciano, Enectali and Strigari, L},
title = {{Implication of neutrino backgrounds on the reach of next generation dark matter direct detection experiments}},
journal = {Phys. Rev. D},
year = {2014},
volume = {89},
number = {2},
pages = {023524},
month = jan,
doi = {10.1103/PhysRevD.89.023524},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-08-22T16:03:25GMT},
date-modified = {2016-11-21T01:03:38GMT},
abstract = {As direct dark matter experiments continue to increase in size, they will become sensitive to neutrinos from astrophysical sources. For experiments that do not have directional sensitivity, coherent neutrino scattering from several sources represents an important background to understand, as it can almost perfectly mimic an authentic weakly interacting massive particle (WIMP) signal. Here we explore in detail the effect of neutrino backgrounds on the discovery potential of WIMPs over the entire mass range of 500 MeV to 10 TeV. We show that, given the theoretical and measured uncertainties on the neutrino backgrounds, direct detection experiments lose sensitivity to light ($\ensuremath{\sim}10\text{ }\text{ }\mathrm{GeV}$) and heavy ($\ensuremath{\sim}100\text{ }\text{ }\mathrm{GeV}$) WIMPs with a spin-independent cross section below ${10}^{\ensuremath{-}45}$ and ${\text{10}}^{\ensuremath{-}49}\text{ }\text{ }{\mathrm{cm}}^{2}$, respectively.},
url = {http://link.aps.org/doi/10.1103/PhysRevD.89.023524},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/D2/D268EE31-3E1F-4BCF-B583-40A68EB14F84.pdf},
file = {{D268EE31-3E1F-4BCF-B583-40A68EB14F84.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/D2/D268EE31-3E1F-4BCF-B583-40A68EB14F84.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.89.023524}}
}

