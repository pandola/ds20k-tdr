@article{Barr:2006ih,
author = {Barr, G D and Robbins, S and Gaisser, T K and Stanev, T},
title = {{Uncertainties in atmospheric neutrino fluxes}},
journal = {Phys. Rev. D},
year = {2006},
volume = {74},
number = {9},
pages = {323},
month = nov,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.74.094009},
language = {English},
rating = {0},
date-added = {2018-11-26T22:59:05GMT},
date-modified = {2018-11-26T22:59:21GMT},
abstract = {An evaluation of the principal uncertainties in the computation of neutrino fluxes produced in cosmic ray showers in the atmosphere is presented. The neutrino flux predictions are needed for comparison with experiment to perform neutrino oscillation studies. The paper concentrates on the main limitations which are due to hadron production uncertainties. It also treats primary cosmic ray flux uncertainties, which are at a lower level. The absolute neutrino fluxes are found to have errors of around 15% in the neutrino energy region important for contained events underground. Large cancellations of these errors occur when ratios of fluxes are considered, in particular, the ${\ensuremath{\nu}}_{\ensuremath{\mu}}/{\overline{\ensuremath{\nu}}}_{\ensuremath{\mu}}$ ratio below ${E}_{\ensuremath{\nu}}=1\text{ }\text{ }\mathrm{GeV}$, the $({\ensuremath{\nu}}_{\ensuremath{\mu}}+{\overline{\ensuremath{\nu}}}_{\ensuremath{\mu}})/({\ensuremath{\nu}}_{e}+{\overline{\ensuremath{\nu}}}_{e})$ ratio below ${E}_{\ensuremath{\nu}}=10\text{ }\text{ }\mathrm{GeV}$ and the up/down ratios above ${E}_{\ensuremath{\nu}}=1\text{ }\text{ }\mathrm{GeV}$ are at the 1% level. A detailed breakdown of the origin of these errors and cancellations is presented.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.74.094009},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/1B/1B462415-E831-401A-BDDD-874690BCB5C1.pdf},
file = {{1B462415-E831-401A-BDDD-874690BCB5C1.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/1B/1B462415-E831-401A-BDDD-874690BCB5C1.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.74.094009}}
}

@article{Essig:2012iv,
author = {Essig, Rouven and Mardon, Jeremy and Volansky, Tomer},
title = {{Direct detection of sub-GeV dark matter}},
journal = {Phys. Rev. D},
year = {2012},
volume = {85},
number = {7},
pages = {076007},
month = apr,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.85.076007},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-02-14T18:17:32GMT},
date-modified = {2018-02-14T18:18:33GMT},
abstract = {Direct detection strategies are proposed for dark matter particles with MeV to GeV mass. In this largely unexplored mass range, dark matter scattering with electrons can cause single-electron ionization signals, which are detectable with current technology. Ultraviolet photons, individual ions, and heat are interesting alternative signals. Focusing on ionization, we calculate the expected dark matter scattering rates and estimate the sensitivity of possible experiments. Backgrounds that may be relevant are discussed. Theoretically interesting models may be within reach using existing data and ongoing direct detection experiments. Significant improvements in sensitivity should be possible with dedicated experiments, opening up a window to new regions in dark matter parameter space.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.85.076007},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/28/28453817-6FCB-4115-9BC3-4D0222CED86B.pdf},
file = {{28453817-6FCB-4115-9BC3-4D0222CED86B.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/28/28453817-6FCB-4115-9BC3-4D0222CED86B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.85.076007}}
}

