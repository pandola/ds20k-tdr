@article{Happer:1984il,
author = {Happer, W and Miron, E and Schaefer, S and Schreiber, D and van Wijngaarden, W A and Zeng, X},
title = {{Polarization of the nuclear spins of noble-gas atoms by spin exchange with optically pumped alkali-metal atoms}},
journal = {Phys. Rev. A},
year = {1984},
volume = {29},
