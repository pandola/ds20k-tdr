month = jul,
publisher = {American Physical Society},
doi = {10.1103/PhysRevLett.109.021301},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-02-14T18:16:35GMT},
date-modified = {2018-02-14T18:18:33GMT},
abstract = {The first direct detection limits on dark matter in the MeV to GeV mass range are presented, using XENON10 data. Such light dark matter can scatter with electrons, causing ionization of atoms in a detector target material and leading to single- or few-electron events. We use $15\text{ }\text{ }\mathrm{kg}\text{ }\mathrm{day}$ of data acquired in 2006 to set limits on the dark-matter\char22{}electron scattering cross section. The strongest bound is obtained at 100 MeV where ${\ensuremath{\sigma}}_{e}{\&}lt;3\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}38}\text{ }\text{ }{\mathrm{cm}}^{2}$ at 90% C.L., while dark-matter masses between 20 MeV and 1 GeV are bounded by ${\ensuremath{\sigma}}_{e}{\&}lt;{10}^{\ensuremath{-}37}\text{ }\text{ }{\mathrm{cm}}^{2}$ at 90% C.L. This analysis provides a first proof of principle that direct detection experiments can be sensitive to dark-matter candidates with masses well below the GeV scale.},
url = {https://link.aps.org/doi/10.1103/PhysRevLett.109.021301},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/BD/BDA18921-B1C3-4E5B-86A1-3C8D3FF37FF7.pdf},
file = {{BDA18921-B1C3-4E5B-86A1-3C8D3FF37FF7.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/BD/BDA18921-B1C3-4E5B-86A1-3C8D3FF37FF7.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevLett.109.021301}}
}

