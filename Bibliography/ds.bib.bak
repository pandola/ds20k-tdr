@article{Cova:1996em,
author = {Cova, Sergio and Ghioni, Massimo and Lacaita, A and Samori, C and Zappa, Franco},
title = {{Avalanche photodiodes and quenching circuits for single-photon detection}},
journal = {Appl. Opt.},
year = {1996},
volume = {35},
number = {12},
pages = {1956--1976},
publisher = {Optical Society of America},
doi = {10.1364/AO.35.001956},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-09-13T14:11:00GMT},
date-modified = {2016-08-05T16:13:43GMT},
abstract = {Avalanche photodiodes, which operate above the breakdown voltage in Geiger mode connected with avalanche-quenching circuits, can be used to detect single photons and are therefore called single-photon avalanche diodes SPAD{\textquoteright}s. Circuit configurations suitable for this operation mode are critically analyzed and their relative merits in photon counting and timing applications are assessed. Simple passive-quenching circuits (PQC{\textquoteright}s), which are useful for SPAD device testing and selection, have fairly limited application. Suitably designed active-quenching circuits (AQC{\textquoteright}s) make it possible to exploit the best performance of SPAD{\textquoteright}s. Thick silicon SPAD{\textquoteright}s that operate at high voltages (250{\textendash}450 V) have photon detection efficiency higher than 50% from 540- to 850-nm wavelength and still {\textasciitilde}3% at 1064 nm. Thin silicon SPAD{\textquoteright}s that operate at low voltages (10{\textendash}50 V) have 45% efficiency at 500 nm, declining to 10% at 830 nm and to as little as 0.1% at 1064 nm. The time resolution achieved in photon timing is 20 ps FWHM with thin SPAD{\textquoteright}s; it ranges from 350 to 150 ps FWHM with thick SPAD{\textquoteright}s. The achieved minimum counting dead time and maximum counting rate are 40 ns and 10 Mcps with thick silicon SPAD{\textquoteright}s, 10 ns and 40 Mcps with thin SPAD{\textquoteright}s. Germanium and III{\textendash}V compound semiconductor SPAD{\textquoteright}s extend the range of photon-counting techniques in the near-infrared region to at least 1600-nm wavelength.},
url = {https://www.osapublishing.org/ao/abstract.cfm?uri=ao-35-12-1956},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/C4/C49942EA-C8DF-4D69-84C3-50F6E692FB09.pdf},
file = {{C49942EA-C8DF-4D69-84C3-50F6E692FB09.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/C4/C49942EA-C8DF-4D69-84C3-50F6E692FB09.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1364/AO.35.001956}}
}

