@article{Coloma:2017vc,
author = {Coloma, Pilar and Gonzalez-Garcia, M C and Maltoni, Michele and Schwetz, Thomas},
title = {{A COHERENT enlightenment of the neutrino Dark Side}},
journal = {arXiv},
year = {2017},
eprint = {1708.02899v1},
eprinttype = {arxiv},
eprintclass = {hep-ph},
month = aug,
annote = {5 pages, 3 figures},
rating = {0},
date-added = {2017-08-21T06:45:52GMT},
date-modified = {2017-09-13T21:55:21GMT},
abstract = {In the presence of non-standard neutrino interactions (NSI), oscillation data are affected by a degeneracy which allows the solar mixing angle to be in the second octant (aka the dark side) and implies a sign flip of the atmospheric mass-squared difference. This leads to an ambiguity in the determination of the ordering of neutrino masses, one of the main goals of the current and future experimental neutrino program. We show that the recent observation of coherent neutrino--nucleus scattering by the COHERENT experiment, in combination with global oscillation data, excludes the NSI degeneracy at the 3.1$\sigma$ (3.6$\sigma$) CL for NSI with up (down) quarks.},
url = {http://arxiv.org/abs/1708.02899v1},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/EC/ECB2AB5F-2B96-4CD5-8F27-652DFA093C02.pdf},
file = {{ECB2AB5F-2B96-4CD5-8F27-652DFA093C02.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/EC/ECB2AB5F-2B96-4CD5-8F27-652DFA093C02.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/7E1B20BA-5E64-4EE5-8EC8-ED32B93DDE86}}
}

