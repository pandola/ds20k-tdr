@article{Lin:2015jw,
author = {Lin, Qing and Fei, Jialing and Gao, Fei and Hu, Jie and Wei, YueHuan and Xiao, Xiang and Wang, HongWei and Ni, KaiXuan},
title = {{Scintillation and ionization responses of liquid xenon to low energy electronic and nuclear recoils at drift fields from 236  V/cmto 3.93  kV/cm}},
journal = {Phys. Rev. D},
year = {2015},
volume = {92},
number = {3},
pages = {032005},
month = aug,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.92.032005},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-01-06T16:30:16GMT},
date-modified = {2018-01-07T17:47:06GMT},
abstract = {We present new measurements of the scintillation and ionization yields in liquid xenon for low energy electronic (about $3\char21{}7\text{ }\text{ }{\mathrm{keV}}_{ee}$) and nuclear recoils (about $8\char21{}20\text{ }\text{ }{\mathrm{keV}}_{nr}$) at different drift fields from $236\text{ }\text{ }\mathrm{V}/\mathrm{cm}$ to $3.93\text{ }\mathrm{kV}/\mathrm{cm}$, using a three-dimensional sensitive liquid xenon time projection chamber with high energy and position resolutions. Our measurement of signal responses to nuclear recoils agrees with predictions from the NEST model. However, our measured ionization (scintillation) yields for electronic recoils are consistently higher (lower) than those from the NEST model by about $5{\mathrm{e}}^{\ensuremath{-}}/{\mathrm{keV}}_{ee}$ ($\mathrm{ph}/{\mathrm{keV}}_{ee}$) at all scanned drift fields. New recombination parameters based on the Thomas-Imel box model are derived from our data. Given the lack of precise measurement of scintillation and ionization yields for low energy electronic recoils in liquid xenon previously, our new measurement provides so far the best available data covering low energy regions at different drift fields for liquid xenon detectors relevant to dark matter searches.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.92.032005},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/F3/F317AA24-AC3A-4290-8713-9AB3B6C8A9E3.pdf},
file = {{F317AA24-AC3A-4290-8713-9AB3B6C8A9E3.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/F3/F317AA24-AC3A-4290-8713-9AB3B6C8A9E3.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.92.032005}}
}

@article{Kuzniak:2018wi,
author = {Ku{\'{z}}niak, Marcin and Broerman, Benjamin},
title = {{Polyethylene naphthalate film as a wavelength shifter in liquid argon detectors}},
journal = {arXiv},
year = {2018},
eprint = {1806.04020v1},
eprinttype = {arxiv},
eprintclass = {physics.ins-det},
month = jun,
annote = {6 pages, 4 figures},
read = {Yes},
rating = {0},
date-added = {2018-06-12T15:47:22GMT},
date-modified = {2018-06-28T05:06:55GMT},
abstract = {Liquid argon-based scintillation detectors are important for dark matter searches and neutrino detection. Argon scintillation light is in the vacuum ultraviolet region, making it hard to be detected by conventional means. Polyethylene naphthalate (PEN), an optically transparent thermoplastic polyester commercially available as large area sheets or rolls, is proposed as an alternative wavelength shifter to the commonly-used tetraphenyl butadiene (TPB). By combining the existing literature data and spectrometer measurements relative to TPB, we conclude that the fluorescence yield and timing of both materials are very close. This suggests that PEN is a suitable replacement for TPB. Advantages of PEN are discussed in the context of scaling-up existing technologies to the next generation of very large ktonne-scale detectors. Its simplicity has a potential to facilitate such scale-ups, revolutionizing the field.},
url = {http://arxiv.org/abs/1806.04020v1},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/58/586BCDD1-20BA-4061-8F3A-42FED9299A84.pdf},
