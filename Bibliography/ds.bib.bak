@article{Maughan:2018bb,
author = {Maughan, Nichole M and Eldib, Mootaz and Faul, David and Conti, Maurizio and Elschot, Mattijs and Kne{\v s}aurek, Karin and Leek, Francesca and Townsend, David and DiFilippo, Frank P and Jackson, Kimberly and Nekolla, Stephan G and Lukas, Mathias and Tapner, Michael and Parikh, Parag J and Laforest, Richard},
title = {{Multi institutional quantitative phantom study of yttrium-90 PET in PET/MRI: the MR-QUEST study}},
journal = {EJNMMI Phys.},
year = {2018},
volume = {5},
number = {1},
pages = {48},
month = apr,
publisher = {Nature Publishing Group},
doi = {10.1186/s40658-018-0206-y},
language = {English},
rating = {0},
date-added = {2018-05-25T06:31:03GMT},
date-modified = {2018-05-25T08:13:25GMT},
abstract = {Yttrium-90 (90Y) radioembolization involves the intra-arterial delivery of radioactive microspheres to treat hepatic malignancies. Though this therapy involves careful pre-treatment planning and imaging, little is known about the precise location of the microspheres once they are administered. Recently, there has been growing interest post-radioembolization imaging using positron-emission tomography (PET) for quantitative dosimetry and identifying lesions that may benefit from additional salvage therapy. In this study, we aim to measure the inter-center variability of 90Y PET measurements as measured on PET/MRI in preparation for a multi-institutional prospective phase I/II clinical trial. Eight institutions participated in this study and followed a standardized phantom filling and imaging protocol. The NEMA NU2-2012 body phantom was filled with 3{\&}nbsp;GBq of 90Y chloride solution. The phantom was imaged for 30{\&}nbsp;min in listmode on a Siemens Biograph mMR non-TOF PET/MRI scanner at five time points across 10{\&}nbsp;days (0.3{\textendash}3.0{\&}nbsp;GBq). Raw PET data were sent to a central site for image reconstruction and data analysis. Images were reconstructed with optimal parameters determined from a previous study. Volumes of interest (VOIs) matching the known sphere diameters were drawn on the vendor-provided attenuation map and propagated to the PET images. Recovery coefficients (RCs) and coefficient of variation of the RCs (COV) were calculated from these VOIs for each sphere size and activity level. Mean RCs ranged from 14.5 to 75.4%, with the lowest mean RC coming from the smallest sphere (10{\&}nbsp;mm) on the last day of imaging (0.16{\&}nbsp;MBq/ml) and the highest mean RC coming from the largest sphere (37{\&}nbsp;mm) on the first day of imaging (2.16{\&}nbsp;MBq/ml). The smaller spheres tended to exhibit higher COVs. In contrast, the larger spheres tended to exhibit lower COVs. COVs from the 37{\&}nbsp;mm sphere were {\&}lt; 25.3% in all scans. For scans with $\ge$ 0.60{\&}nbsp;MBq/ml, COVs were $\le$ 25% in spheres $\ge$ 22{\&}nbsp;mm. However, for all other spheres sizes and activity levels, COVs were usually {\&}gt; 25%. Post-radioembolization dosimetry of lesions or other VOIs $\ge$ 22{\&}nbsp;mm in diameter can be consistently obtained ({\&}lt; 25% variability) at a multi-institutional level using PET/MRI for any clinically significant activity for 90Y radioembolization.},
url = {https://ejnmmiphys.springeropen.com/articles/10.1186/s40658-018-0206-y},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/1A/1AAC621D-5E62-4801-8F53-8C79635C7151.pdf},
file = {{1AAC621D-5E62-4801-8F53-8C79635C7151.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/1A/1AAC621D-5E62-4801-8F53-8C79635C7151.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/s40658-018-0206-y}}
}

