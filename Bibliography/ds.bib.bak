@article{Galbiati:2005fo,
author = {Galbiati, Cristiano and Beacom, John F},
title = {{Measuring the cosmic ray muon-induced fast neutron spectrum by (\emph{n},\emph{p}) isotope production reactions in underground detectors}},
journal = {Phys. Rev. C},
year = {2005},
volume = {72},
number = {2},
pages = {025807},
month = aug,
doi = {10.1103/PhysRevC.72.025807},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-08-18T19:16:19GMT},
date-modified = {2015-09-07T19:53:16GMT},
abstract = {While cosmic ray muons themselves are relatively easy to veto in underground detectors, their interactions with nuclei create more insidious backgrounds via (i) the decays of long-lived isotopes produced by muon-induced spallation reactions inside the detector, (ii) spallation reactions initiated by fast muon-induced neutrons entering from outside the detector, and (iii) nuclear recoils initiated by fast muon-induced neutrons entering from outside the detector. These backgrounds, which are difficult to veto or shield against; are very important for solar, reactor, dark matter, and other underground experiments, especially as increased sensitivity is pursued. We used FLUKA to calculate the production rates and spectra of all prominent secondaries produced by cosmic ray muons, in particular focusing on secondary neutrons, because of their importance. Since the neutron spectrum is steeply falling, the total neutron production rate is sensitive to just the relatively soft neutrons and not the fast-neutron component. We show that the neutron spectrum in the range \ensuremath{\sim}10\char21{}100 MeV can instead be probed by the $(n,p)$-induced isotope production rates $^{12}\mathrm{C}$$(n,p)$$^{12}\mathrm{B}$ and $^{16}\mathrm{O}$$(n,p)$$^{16}\mathrm{N}$ in oil- and water-based detectors. The result for $^{12}\mathrm{B}$ is in good agreement with the recent KamLAND measurement. Besides testing the calculation of muon secondaries, these results are also of practical importance, since $^{12}\mathrm{B}$ (${T}_{1/2}=20.2$ ms, $Q=13.4\phantom{\rule{0.3em}{0ex}}\text{MeV}$) and $^{16}\mathrm{N}$ (${T}_{1/2}=7.13$ s, $Q=10.4\phantom{\rule{0.3em}{0ex}}\text{MeV}$) are among the dominant spallation backgrounds in these detectors.},
url = {http://link.aps.org/doi/10.1103/PhysRevC.72.025807},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/9A/9A21EFDC-322B-403B-8F50-ECCBD9C31838.pdf},
file = {{9A21EFDC-322B-403B-8F50-ECCBD9C31838.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/9A/9A21EFDC-322B-403B-8F50-ECCBD9C31838.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevC.72.025807}}
}

@article{Lewis:2004ii,
author = {Lewis, Matthew J and Freese, Katherine},
title = {{Phase of the annual modulation as a tool for determining the mass of the weakly interacting massive particle}},
journal = {Phys. Rev. D},
year = {2004},
volume = {70},
number = {4},
pages = {043501},
month = aug,
publisher = {American Physical Society},
doi = {10.1103/PhysRevD.70.043501},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-02-28T16:26:55GMT},
date-modified = {2017-02-28T16:30:27GMT},
abstract = {The count rate of weakly interacting massive particle (WIMP) dark matter candidates in direct detection experiments experiences an annual modulation due to the Earth's motion around the Sun. In the standard isothermal halo model, the signal peaks near June 2nd at high recoil energies; however, the signal experiences a phase reversal and peaks in December at low energy recoils. We show that this phase reversal may be used to determine the WIMP mass. If an annual modulation were observed with the usual phase (i.e., peaking on June 2nd) in the lowest accessible energy recoil bins of the DAMA, CDMS-II, CRESST-II, EDELWEISS-II, GENIUS-TF, ZEPLIN-II, XENON, or ZEPLIN-IV detectors, one could immediately place upper bounds on the WIMP mass of 103, 48, 6, 97, 10, 52, 29, and 29 GeV, respectively. In addition, detectors with adequate energy resolution and sufficiently low recoil energy thresholds may determine the crossover recoil energy at which the phase reverses, thereby obtaining an independent measurement of the WIMP mass. We study the capabilities of various detectors, and find that CRESST-II, ZEPLIN-II, and GENIUS-TF should be able to observe the phase reversal in a few years of runtime, and can thus determine the mass of the WIMP if it is $O(100\mathrm{GeV}).$ Xenon based detectors with 1000 kg (XENON and ZEPLIN-IV) and with energy recoil thresholds of a few keV require 25 kg yr exposure, which will be readily attained in upcoming experiments.},
url = {http://link.aps.org/doi/10.1103/PhysRevD.70.043501},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/93/93D5B10D-2104-492E-A9E1-5237D080C2CE.pdf},
file = {{93D5B10D-2104-492E-A9E1-5237D080C2CE.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/93/93D5B10D-2104-492E-A9E1-5237D080C2CE.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.70.043501}}
}

