abstract = {The Pauli exclusion principle (PEP) has been tested for nucleons ($n,p$) in $^{12}\mathrm{C}$ with the Borexino detector. The approach consists of a search for $\ensuremath{\gamma}$, $n$, $p$, and ${\ensuremath{\beta}}^{\ifmmode\pm\else\textpm\fi{}}$ emitted in a non-Paulian transition of $1{P}_{3/2}$-shell nucleons to the filled $1{S}_{1/2}$ shell in nuclei. Due to the extremely low background and the large mass ($278$ tons) of the Borexino detector, the following most stringent up-to-date experimental bounds on PEP violating transitions of nucleons have been established: $\ensuremath{\tau}({}^{12}\mathrm{C}\ensuremath{\rightarrow}{}^{12}\stackrel{{\textasciitilde}}{\mathrm{C}}+\ensuremath{\gamma})\ensuremath{\geqslant}5.0\ifmmode\times\else\texttimes\fi{}{10}^{31}$ yr, $\ensuremath{\tau}({}^{12}\mathrm{C}\ensuremath{\rightarrow}{}^{11}\stackrel{{\textasciitilde}}{\mathrm{B}}+p)\ensuremath{\geqslant}8.9\ifmmode\times\else\texttimes\fi{}{10}^{29}$ yr, $\ensuremath{\tau}({}^{12}\mathrm{C}\ensuremath{\rightarrow}{}^{11}\stackrel{{\textasciitilde}}{\mathrm{C}}+n)\ensuremath{\geqslant}3.4\ifmmode\times\else\texttimes\fi{}{10}^{30}$ yr, $\ensuremath{\tau}({}^{12}\mathrm{C}\ensuremath{\rightarrow}{}^{12}\stackrel{{\textasciitilde}}{\mathrm{N}}+{e}^{\ensuremath{-}}+\stackrel{{\textasciitilde}}{{\ensuremath{\nu}}_{e}})\ensuremath{\geqslant}3.1\ifmmode\times\else\texttimes\fi{}{10}^{30}$ yr, and $\ensuremath{\tau}({}^{12}\mathrm{C}\ensuremath{\rightarrow}{}^{12}\stackrel{{\textasciitilde}}{\mathrm{B}}+{e}^{+}+{\ensuremath{\nu}}_{e})\ensuremath{\geqslant}2.1\ifmmode\times\else\texttimes\fi{}{10}^{30}$ yr, all at $90%$ C.L. The corresponding upper limits on the relative strengths for the searched non-Paulian electromagnetic, strong and weak transitions have been estimated as ${\ensuremath{\delta}}_{\ensuremath{\gamma}}^{2}\ensuremath{\leqslant}2.2\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}57}$, ${\ensuremath{\delta}}_{N}^{2}\ensuremath{\leqslant}4.1\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}60}$, and ${\ensuremath{\delta}}_{\ensuremath{\beta}}^{2}\ensuremath{\leqslant}2.1\ifmmode\times\else\texttimes\fi{}{10}^{\ensuremath{-}35}$.},
url = {http://link.aps.org/doi/10.1103/PhysRevC.81.034317},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/F6/F6F62839-E7B4-4627-A25F-0FC08B7E0D22.pdf},
file = {{F6F62839-E7B4-4627-A25F-0FC08B7E0D22.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/F6/F6F62839-E7B4-4627-A25F-0FC08B7E0D22.pdf:application/pdf}},
