@article{Bertone:2019vd,
author = {Bertone, Gianfranco and Coogan, Adam and Gaggero, Daniele and Kavanagh, Bradley J and Weniger, Christoph},
title = {{Primordial Black Holes as Silver Bullets for New Physics at the Weak Scale}},
journal = {arXiv},
year = {2019},
eprint = {1905.01238v1},
eprinttype = {arxiv},
eprintclass = {hep-ph},
month = may,
annote = {10 pages, 3 figures. Code available at https://github.com/adam-coogan/pbhs_vs_wimps , archived at https://zenodo.org/badge/latestdoi/169754838},
read = {Yes},
rating = {0},
date-added = {2019-05-21T13:17:42GMT},
date-modified = {2019-06-15T11:31:04GMT},
abstract = {Observational constraints on gamma rays produced by the annihilation of weakly interacting massive particles around primordial black holes (PBHs) imply that these two classes of Dark Matter candidates cannot coexist. We show here that the successful detection of one or more PBHs by radio searches (with the Square Kilometer Array) and gravitational waves searches (with LIGO/Virgo and the upcoming Einstein Telescope) would set extraordinarily stringent constraints on virtually all weak-scale extensions of the Standard Model with stable relics, including those predicting a WIMP abundance much smaller than that of Dark Matter. Upcoming PBHs searches have in particular the potential to rule out almost the entire parameter space of popular theories such as the minimal supersymmetric standard model and scalar singlet Dark Matter.},
url = {http://arxiv.org/abs/1905.01238v1},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/E1/E1AC4BDE-190F-41FD-8D0E-2BFD5A8938CD.pdf},
file = {{E1AC4BDE-190F-41FD-8D0E-2BFD5A8938CD.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/E1/E1AC4BDE-190F-41FD-8D0E-2BFD5A8938CD.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/EF3165BB-D6BA-41F1-A00D-4AC4CB07D15B}}
}

