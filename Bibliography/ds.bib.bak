date-added = {2015-09-19T13:45:02GMT},
date-modified = {2015-09-21T13:44:22GMT},
abstract = {The theory of spin exchange between optically pumped alkali-metal atoms and noble-gas nuclei is presented. Spin exchange with heavy noble gases is dominated by interactions in long-lived van der Waals molecules. The main spin interactions are assumed to be the spin-rotation interactions $\ensuremath{\gamma}\stackrel{\ensuremath{\rightarrow}}{\mathrm{N}}\ifmmode\cdot\else\textperiodcentered\fi{}\stackrel{\ensuremath{\rightarrow}}{\mathrm{S}}$ between the rotational angular momentum $\stackrel{\ensuremath{\rightarrow}}{\mathrm{N}}$ of the alkali-metal\char22{}noble-gas pair and the electron spin $\stackrel{\ensuremath{\rightarrow}}{\mathrm{S}}$ of the alkali-metal atom, and the contact hyperfine interaction $\ensuremath{\alpha}\stackrel{\ensuremath{\rightarrow}}{\mathrm{K}}\ifmmode\cdot\else\textperiodcentered\fi{}\stackrel{\ensuremath{\rightarrow}}{\mathrm{S}}$ between the nuclear spin $\stackrel{\ensuremath{\rightarrow}}{\mathrm{K}}$ of the noble-gas atom and the electron spin $\stackrel{\ensuremath{\rightarrow}}{\mathrm{S}}$. Arbitrary values for $K$ and for the nuclear spin $I$ of the alkali-metal atom are assumed. Precise formal expressions for spin transfer coefficients are given along with convenient approximations based on a perturbation expansion in powers of ${(\frac{\ensuremath{\alpha}}{\ensuremath{\gamma}N})}^{2}$, a quantity which has been shown to be small by experiment.},
url = {http://link.aps.org/doi/10.1103/PhysRevA.29.3092},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/F8/F83F80E4-250D-4E18-9EBF-F51D841AE62D.pdf},
file = {{F83F80E4-250D-4E18-9EBF-F51D841AE62D.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/F8/F83F80E4-250D-4E18-9EBF-F51D841AE62D.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevA.29.3092}}
}

