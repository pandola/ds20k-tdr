doi = {10.1103/PhysRevD.96.103007},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-12-31T18:15:32GMT},
date-modified = {2017-12-31T18:23:05GMT},
abstract = {The dependence of the light and charge yield of liquid xenon on the applied electric field and recoil energy is important for dark matter detectors using liquid xenon time projections chambers. Few measurements have been made of this field dependence at recoil energies less than 10 keV. In this paper, we present results of such measurements using a specialized detector. Recoil energies are determined via the Compton coincidence technique at four drift fields relevant for liquid xenon dark matter detectors: 0.19, 0.48, 1.02, and $2.32\text{ }\text{ }\mathrm{kV}/\mathrm{cm}$. Mean recoil energies down to 1 keV were measured with unprecedented precision. We find that the charge and light yield are anticorrelated above $\ensuremath{\sim}3\text{ }\text{ }\mathrm{keV}$ and that the field dependence becomes negligible below $\ensuremath{\sim}6\text{ }\text{ }\mathrm{keV}$. However, below 3 keV, we find a charge yield significantly higher than expectation and a reconstructed energy deviating from linearity.},
url = {https://link.aps.org/doi/10.1103/PhysRevD.96.103007},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/8A/8AB04BD0-C047-4A17-9137-4E3718EBD4A7.pdf},
file = {{8AB04BD0-C047-4A17-9137-4E3718EBD4A7.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/8A/8AB04BD0-C047-4A17-9137-4E3718EBD4A7.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.96.103007}}
}

@article{TheCMSCollaboration:2018wx,
author = {{The CMS Collaboration}},
title = {{Search for dark matter in final states with a leptoquark and missing transverse momentum in proton-proton collisions at 13 TeV}},
journal = {CERN Doc. Serv.},
year = {2018},
volume = {2628478},
read = {Yes},
rating = {0},
date-added = {2018-07-28T15:25:49GMT},
date-modified = {2018-07-28T16:49:07GMT},
abstract = {A search for dark matter is presented in proton-proton collisions at a center-of-mass energy of $\sqrt{s}=13{\textasciitilde}\mathrm{TeV}$ using events with at least one high transverse momentum ($p_{\mathrm{T}}$) muon, at least one high $p_{\mathrm{T}}$ jet, and large missing transverse momentum. The search is performed in the context of the so-called coannihilation codex, a new paradigm that introduces novel scenarios for dark matter production. The data were collected with the CMS detector at the CERN LHC in 2016 and 2017, and correspond to an integrated luminosity of $77.4{\textasciitilde}\mathrm{fb}^{-1}$. In the examined scenario, a pair of scalar leptoquarks is assumed to be produced. One leptoquark decays to a muon and a jet while the other decays to dark matter and soft standard model particles. The dark matter signature is given by a peak at the leptoquark mass in the invariant mass distribution of the highest $p_{\mathrm{T}}$ muon and jet. The data are observed to agree with the predictions from the standard model. Leptoquarks with masses up to $1160{\textasciitilde}\mathrm{GeV}$ are excluded.},
url = {http://cds.cern.ch/record/2628478?ln=en},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/25/25873DDA-9F46-4E4C-A0AB-62B1D619CD7C.pdf},
file = {{25873DDA-9F46-4E4C-A0AB-62B1D619CD7C.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/25/25873DDA-9F46-4E4C-A0AB-62B1D619CD7C.pdf:application/pdf;25873DDA-9F46-4E4C-A0AB-62B1D619CD7C.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/25/25873DDA-9F46-4E4C-A0AB-62B1D619CD7C.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/B462377E-E9C8-40FA-845A-145146EFDA3A}}
}

