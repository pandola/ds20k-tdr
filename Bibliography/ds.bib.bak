@article{DArienzo:2017jw,
author = {D{\textquoteright}Arienzo, Marco and Cox, Maurice},
title = {{Uncertainty Analysis in the Calibration of an Emission Tomography System for Quantitative Imaging}},
journal = {Computational and Mathematical Methods in Medicine},
year = {2017},
volume = {2017},
number = {4},
pages = {1--9},
publisher = {Hindawi},
doi = {10.1155/2017/9830386},
language = {English},
rating = {0},
date-added = {2018-05-25T08:12:45GMT},
date-modified = {2018-05-25T08:13:25GMT},
abstract = {It is generally acknowledged that calibration of the imaging system (be it a SPECT or a PET scanner) is one of the critical components associated with in vivo activity quantification in nuclear medicine. The system calibration is generally performed through the acquisition of a source with a known amount of radioactivity. The decay-corrected calibration factor is the {\&}{\#}x201c;output{\&}{\#}x201d; quantity in a measurement model for the process. This quantity is a function of a number of {\&}{\#}x201c;input{\&}{\#}x201d; variables, including total counts in the volume of interest (VOI), radionuclide activity concentration, source volume, acquisition duration, radionuclide half-life, and calibration time of the radionuclide. Uncertainties in the input variables propagate through the calculation to the {\&}{\#}x201c;combined{\&}{\#}x201d; uncertainty in the output quantity. In the present study, using the general formula given in the GUM (Guide to the Expression of Uncertainty in Measurement) for aggregating uncertainty components, we derive a practical relation to assess the combined standard uncertainty for the calibration factor of an emission tomography system. At a time of increasing need for accuracy in quantification studies, the proposed approach has the potential to be easily implemented in clinical practice.},
url = {https://www.hindawi.com/journals/cmmm/2017/9830386/},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/ED/ED888C4F-A015-4DDF-9B07-AFE9C4BF1AE3.pdf},
file = {{ED888C4F-A015-4DDF-9B07-AFE9C4BF1AE3.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/ED/ED888C4F-A015-4DDF-9B07-AFE9C4BF1AE3.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1155/2017/9830386}}
}

@article{Amole:2015eu,
