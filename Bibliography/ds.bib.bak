@article{Goetzke:2017ic,
author = {Goetzke, L W and Aprile, Elena and Anthony, M and Plante, Guillame and Weber, M},
title = {{Measurement of light and charge yield of low-energy electronic recoils in liquid xenon}},
journal = {Phys. Rev. D},
year = {2017},
volume = {96},
number = {10},
pages = {103007},
