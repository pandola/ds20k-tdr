@article{Chiao:2004ez,
author = {Chiao, Raymond Y and Speliotopoulos, Achilles D},
title = {{Towards MIGO, the matter-wave interferometric gravitational-wave observatory, and the intersection of quantum mechanics with general relativity}},
journal = {J. Mod. Optics},
year = {2004},
volume = {51},
number = {6-7},
pages = {861--899},
month = apr,
publisher = { Taylor {\&} Francis Group},
doi = {10.1080/09500340408233603},
language = {English},
rating = {0},
date-added = {2015-09-21T13:35:32GMT},
date-modified = {2016-08-05T16:13:43GMT},
abstract = {Abstract A dynamical non-Euclidean spacetime geometry in general relativity theory implies the possibility of gravitational radiation. Here we explore novel methods of detecting such radiation from astrophysical sources by means of matter-wave interferometers (MIGOs), using atomic beams emanating from supersonic atomic sources that are further cooled and collimated by means of optical molasses. While the sensitivities of such MIGOs compare favorably with LIGO and LISA, the sizes of MIGOs can be orders of magnitude smaller, and their bandwidths wider. Using a pedagogical approach, we place this problem into the broader context of problems at the intersection of quantum mechanics with general relativity.},
url = {http://www.tandfonline.com/doi/abs/10.1080/09500340408233603},
local-url = {file://localhost/Users/cris/Dropbox/Library/Library.papers3/Files/65/651999E3-E4F2-4104-9B77-0E28C4BA60BD.pdf},
file = {{651999E3-E4F2-4104-9B77-0E28C4BA60BD.pdf:/Users/cris/Dropbox/Library/Library.papers3/Files/65/651999E3-E4F2-4104-9B77-0E28C4BA60BD.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1080/09500340408233603}}
}

