@article{Acciarri:2010gm,
author = {Acciarri, Roberto and Antonello, Maddalena and Baibussinov, Bagdat and Baldo-Ceolin, Milla and Benetti, Piero and Calaprice, Frank P and Calligarich, Elio and Cambiaghi, Mario and Canci, Nicola and Carbonara, Francesco and Cavanna, Flavio and Centro, Sandro and Cocco, Alfredo G and Di Pompeo, Francesco and Fiorillo, Giuliana and Galbiati, Cristiano and Gallo, Valentina and Grandi, Luca and Meng, Guang and Modena, Ivo and Montanari, Claudio and Palamara, Ornella and Pandola, Luciano and Piano Mortari, Giovanni B. and Pietropaolo, Francesco and Raselli, Gian Luca and Roncadelli, Marco and Rossella, Massimo and Rubbia, Carlo and Segreto, Ettore and Szelc, Andrzej M and Ventura, Sandro and Vignoli, Chiara},
