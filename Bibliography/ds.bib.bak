title = {{Cosmogenic Backgrounds in Borexino at 3800 m water-equivalent depth}},
journal = {JCAP},
year = {2013},
volume = {1308},
number = {08},
pages = {049--049},
month = aug,
publisher = {IOP Publishing},
affiliation = {The Borexino Collaboration},
doi = {10.1088/1475-7516/2013/08/049},
language = {English},
read = {Yes},
