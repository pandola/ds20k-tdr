author = {Goldan, A H and Rowlands, John A and Tousignant, O and Karim, K S},
title = {{Unipolar time-differential charge sensing in non-dispersive amorphous solids}},
journal = {J. Appl. Phys.},
year = {2013},
volume = {113},
